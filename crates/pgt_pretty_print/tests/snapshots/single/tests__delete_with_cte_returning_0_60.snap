WITH stale AS (SELECT
  id
FROM
  sessions
WHERE last_seen < NOW() - CAST('30 days' AS INTERVAL))
DELETE FROM sessions
USING stale
WHERE sessions.id = stale.id
RETURNING sessions.id;
